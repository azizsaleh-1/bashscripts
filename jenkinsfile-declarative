pipeline{
    agent any

    environment {
        project = "Test"
    }
    stages {
        stage('build'){
            steps{
                sh"sh untilloop.sh"
            }
        }
        stage('Array'){
            steps{
                sh "sh array"
            }
        }
    }
    post {
        success {
            echo "success"
            sh 'mail -s "Success" mebiru2051@gmail.com'
        }
        failure {
            echo "Failure"
            sh 'mail -s "Failed" mebiru2051@gmail.com'
        }
        always {
            deleteDir()
        }
    }
}